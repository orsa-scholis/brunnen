<h2><%= administrator_signed_in? ? t('root.all_surveys') : t('root.active_surveys') %></h2>
<% if @surveys.empty? %>
  <h5 class="text-muted"><%= t('root.no_active_surveys') %></h5>
<% end %>
<div class="list-group">
  <% @surveys.each do |survey| %>
    <%= survey_list_link(survey, @submission_tracking) %>
  <% end %>
</div>
